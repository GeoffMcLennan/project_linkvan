<%= form_for(@facility) do |f| %>
  <% if @facility.errors.any? %>
  <h3 style="color:red">Oops, did you remember to "Search" the address? Name missing?</h3>
  <h4>Errors: <%= @facility.errors.full_messages.to_sentence %></h4>
  <% end %>
  <p>
    <strong> NOTE: DO NOT PRESS ENTER WHEN FILLING IN FIELDS. USE "CREATE FACILITY"/"UPDATE FACILITY" BUTTON AT BOTTOM OF PAGE.</strong><br/><br/>
    <%= f.label :name %>&nbsp;&nbsp;&nbsp;<%= f.text_field :name, autofocus: true %>
  </p>
  <hr>
  <p>
    <%= f.label :welcomes %>&nbsp;&nbsp;&nbsp;<%= f.select :welcomes, [['All'], ['Women'], ['Men']] %>
  </p>
  <hr>
  <p>
    <%= f.label :suitability %> - Select All Relevant Categories<br/>
    <strong>Note: Suitability is currently not used; however, future designs of the web app may use this information to better search and sort facilities.</strong><br>
    <%= f.hidden_field :suitability %><br>

    <% if @facility.suitability.nil? %>
      <input type="checkbox" id="chcheck" name="Children" value="Children" onclick="chclicked()">Children &nbsp;&nbsp;
      <input type="checkbox" id="yocheck" name="Youth" value="Youth" onclick="yoclicked()">Youth &nbsp;&nbsp;
      <input type="checkbox" id="adcheck" name="Adults" value="Adults" onclick="adclicked()">Adults &nbsp;&nbsp;
      <input type="checkbox" id="secheck" name="Seniors" value="Seniors" onclick="seclicked()">Seniors &nbsp;&nbsp;
    <%else%>

    <% if @facility.suitability.include? "Children" %>
      <input type="checkbox" id="chcheck" name="Children" value="Children" onclick="chclicked()" checked>Children &nbsp;&nbsp;
    <%else%>
      <input type="checkbox" id="chcheck" name="Children" value="Children" onclick="chclicked()">Children &nbsp;&nbsp;
    <%end%>

    <% if @facility.suitability.include? "Youth" %>
      <input type="checkbox" id="yocheck" name="Youth" value="Youth" onclick="yoclicked()" checked>Youth &nbsp;&nbsp;
    <%else%>
      <input type="checkbox" id="yocheck" name="Youth" value="Youth" onclick="yoclicked()">Youth &nbsp;&nbsp;
    <%end%>

    <% if @facility.suitability.include? "Adults" %>
      <input type="checkbox" id="adcheck" name="Adults" value="Adults" onclick="adclicked()" checked>Adults &nbsp;&nbsp;
    <%else%>
      <input type="checkbox" id="adcheck" name="Adults" value="Adults" onclick="adclicked()">Adults &nbsp;&nbsp;
    <%end%>

    <% if @facility.suitability.include? "Seniors" %>
      <input type="checkbox" id="secheck" name="Seniors" value="Seniors" onclick="seclicked()" checked>Seniors &nbsp;&nbsp;
    <%else%>
      <input type="checkbox" id="secheck" name="Seniors" value="Seniors" onclick="seclicked()">Seniors &nbsp;&nbsp;
    <%end%>


    <%end%>


  </p>
  <hr>
  <p>
    <%= f.label :services %> - Select All Relevant Categories<br/>


    <%= f.hidden_field :services%><br>
    <% if @facility.services.nil? %>
      <input type="checkbox" id="scheck" name="Shelter" value="Shelter" onclick="servclicked('scheck')">Shelter &nbsp;&nbsp;
      <input type="checkbox" id="fcheck" name="Food" value="Food" onclick="servclicked('fcheck')">Food &nbsp;&nbsp;
      <input type="checkbox" id="mcheck" name="Medical" value="Medical" onclick="servclicked('mcheck')">Medical &nbsp;&nbsp;
      <input type="checkbox" id="hcheck" name="Hygiene" value="Hygiene" onclick="servclicked('hcheck')">Hygiene &nbsp;&nbsp;
      <input type="checkbox" id="tcheck" name="Technology" value="Technology" onclick="servclicked('tcheck')">Technology &nbsp;&nbsp;
      <input type="checkbox" id="acheck" name="Advocacy" value="Advocacy" onclick="servclicked('acheck')">Advocacy &nbsp;&nbsp;
    <%else%>

    <% if @facility.services.include? "Shelter" %>
      <input type="checkbox" id="scheck" name="Shelter" value="Shelter" onclick="servclicked('scheck')" checked>Shelter &nbsp;&nbsp;
    <%else%>
      <input type="checkbox" id="scheck" name="Shelter" value="Shelter" onclick="servclicked('scheck')">Shelter &nbsp;&nbsp;
    <%end%>

    <% if @facility.services.include? "Food" %>
      <input type="checkbox" id="fcheck" name="Food" value="Food" onclick="servclicked('fcheck')" checked>Food &nbsp;&nbsp;
    <%else%>
      <input type="checkbox" id="fcheck" name="Food" value="Food" onclick="servclicked('fcheck')">Food &nbsp;&nbsp;
    <%end%>

    <% if @facility.services.include? "Medical" %>
