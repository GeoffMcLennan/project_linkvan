<script>

var chcheck;
var yocheck;
var adcheck;
var secheck;

var scheck;
var fcheck;
var mcheck;
var hcheck;
var tcheck;
var acheck;
var finalservices = 
  <% if @facility.services.nil? %>
    [];
  <% else %>
    <%= raw @facility.services.split(' ').to_json %>;
  <% end %>  
var finalsuitability = [];

function chclicked(){
  var index;
  chcheck = document.getElementById('chcheck');
  if(chcheck.checked){
    finalsuitability.push(chcheck.value);
  }else{
    index = finalsuitability.indexOf(chcheck.value);
    if (index > -1) {
      finalsuitability.splice(index, 1);
    }
  }
  document.getElementById('facility_suitability').value = finalsuitability.join(' ');
}

function yoclicked(){
  var index;
  yocheck = document.getElementById('yocheck');
  if(yocheck.checked){
    finalsuitability.push(yocheck.value);
  }else{
    index = finalsuitability.indexOf(yocheck.value);
    if (index > -1) {
      finalsuitability.splice(index, 1);
    }
  }
  document.getElementById('facility_suitability').value = finalsuitability.join(' ');
}

function adclicked(){
  var index;
  adcheck = document.getElementById('adcheck');
  if(adcheck.checked){
    finalsuitability.push(adcheck.value);
  }else{
    index = finalsuitability.indexOf(adcheck.value);
    if (index > -1) {
      finalsuitability.splice(index, 1);
    }
  }
  document.getElementById('facility_suitability').value = finalsuitability.join(' ');
}

function seclicked(){
  var index;
  secheck = document.getElementById('secheck');
  if(secheck.checked){
    finalsuitability.push(secheck.value);
  }else{
    index = finalsuitability.indexOf(secheck.value);
    if (index > -1) {
      finalsuitability.splice(index, 1);
    }
  }
  document.getElementById('facility_suitability').value = finalsuitability.join(' ');
}

function servclicked(service) {
  var index;
  check = document.getElementById(service);
  if (check.checked) {
    finalservices.push(check.value);
  } else {
    index = finalservices.indexOf(check.value);
    if (index > -1) {
      finalservices.splice(index, 1);
    }
  }
  document.getElementById('facility_services').value = finalservices.join(' ');
}

var monhours = document.getElementById("mon-hours");
var tueshours = document.getElementById("tues-hours");
var wedhours = document.getElementById("wed-hours");
var thurshours = document.getElementById("thurs-hours");
var frihours = document.getElementById("fri-hours");
var sathours = document.getElementById("sat-hours");
var sunhours = document.getElementById("sun-hours");

var monhours2 = document.getElementById("mon-hours-2");
var tueshours2 = document.getElementById("tues-hours-2");
var wedhours2 = document.getElementById("wed-hours-2");
var thurshours2 = document.getElementById("thurs-hours-2");
var frihours2 = document.getElementById("fri-hours-2");
var sathours2 = document.getElementById("sat-hours-2");
var sunhours2 = document.getElementById("sun-hours-2");


//open and closed all days
var oadmon = document.getElementById('facility_open_all_day_mon');
oadmon.setAttribute("onclick", "validate_mon()");
var cadmon = document.getElementById('facility_closed_all_day_mon');
cadmon.setAttribute("onclick", "validate_mon()");

var oadtues = document.getElementById('facility_open_all_day_tues');
oadtues.setAttribute("onclick", "validate_tues()");
var cadtues = document.getElementById('facility_closed_all_day_tues');
